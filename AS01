import random

def bubblesort(arr):
    x = len(arr)


    for z in range(x):

        for y in range(0, x-1):

            if arr[y] > arr[y+1] :
                arr[y], arr[y+1] = arr[y+1], arr[y]

arr = []
for i in range(8):

        rabot = random.randint(0, 99)
        rabot = str(rabot)
        arr.append(rabot)


bubblesort(arr)

print("Sorted array is:")
for i in arr:
    print (i)
